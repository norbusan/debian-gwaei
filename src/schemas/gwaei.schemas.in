<?xml version="1.0" encoding="utf-8"?><!-- -*- indent-tabs-mode: nil -*- -->
<gconfschemafile>
   <schemalist>

      <!-- General settings -->
      <schema>
        <key>/schemas/apps/gwaei/fonts/use_global_document_font</key>
        <applyto>/apps/gwaei/fonts/use_global_document_font</applyto>
        <owner>gwaei</owner>
        <type>bool</type>
        <default>TRUE</default>
        <locale name="C">
	      <short>Use the global document font</short>
	      <long>Makes the program use the global document font instead of the custom one.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/fonts/custom_document_font</key>
        <applyto>/apps/gwaei/fonts/custom_document_font</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>Sans 10</default>
        <locale name="C">
	      <short>Custom document font</short>
	      <long>A document font to use instead of the global one.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/fonts/magnification</key>
        <applyto>/apps/gwaei/fonts/magnification</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Results font size</short>
	      <long>Enlarges the document font by the step given here.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/toolbar_show</key>
        <applyto>/apps/gwaei/toolbar_show</applyto>
        <owner>gwaei</owner>
        <type>bool</type>
        <default>FALSE</default>
        <locale name="C">
	      <short>Toolbar show status</short>
	      <long>Determines if the toolbar is hidden or not.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/less_relevant_results_show</key>
        <applyto>/apps/gwaei/less_relevant_results_show</applyto>
        <owner>gwaei</owner>
        <type>bool</type>
        <default>TRUE</default>
        <locale name="C">
	      <short>Toolbar show status</short>
	      <long>Determines if the toolbar is hidden or not.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/query_katakana_to_hiragana</key>
        <applyto>/apps/gwaei/query_katakana_to_hiragana</applyto>
        <owner>gwaei</owner>
        <type>bool</type>
        <default>TRUE</default>
        <locale name="C">
	      <short>Convert Katakana queries to Hiragana</short>
	      <long>Will also search for the Hiragana version of a Katakana query.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/query_hiragana_to_katakana</key>
        <applyto>/apps/gwaei/query_hiragana_to_katakana</applyto>
        <owner>gwaei</owner>
        <type>bool</type>
        <default>TRUE</default>
        <locale name="C">
	      <short>Convert Hiragana queries to Katakana</short>
	      <long>Will also search for the Katakana version of a Hiragana query.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/query_romanji_to_kana</key>
        <applyto>/apps/gwaei/query_romanji_to_kana</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>2</default>
        <locale name="C">
	      <short>Convert Romanji queries to Kana</short>
	      <long>Will also search for Kana versions of a Romanji query.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/query_spellcheck</key>
        <applyto>/apps/gwaei/query_spellcheck</applyto>
        <owner>gwaei</owner>
        <type>bool</type>
        <default>TRUE</default>
        <locale name="C">
	      <short>Check the spelling of queries as they are typed</short>
	      <long>Checks the spelling of search queries as the user types them in.</long>
        </locale>
      </schema>






      <!-- Main window attributes -->
      <schema>
        <key>/schemas/apps/gwaei/main_window/x</key>
        <applyto>/apps/gwaei/main_window/x</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored x position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/main_window/y</key>
        <applyto>/apps/gwaei/main_window/y</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored y position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/main_window/width</key>
        <applyto>/apps/gwaei/main_window/width</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored width position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/main_window/height</key>
        <applyto>/apps/gwaei/main_window/height</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored height position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <!-- Radicals window attributes -->
      <schema>
        <key>/schemas/apps/gwaei/radicals_window/x</key>
        <applyto>/apps/gwaei/radicals_window/x</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored x position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/radicals_window/y</key>
        <applyto>/apps/gwaei/radicals_window/y</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored y position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/radicals_window/width</key>
        <applyto>/apps/gwaei/radicals_window/width</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored width position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/radicals_window/height</key>
        <applyto>/apps/gwaei/radicals_window/height</applyto>
        <owner>gwaei</owner>
        <type>int</type>
        <default>0</default>
        <locale name="C">
	      <short>Stored height position of this window</short>
	      <long>Used for restoring the position of the window.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/dictionary/english_source</key>
        <applyto>/apps/gwaei/dictionary/english_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@ENGLISH_DEFAULT_URI@</default>
        <locale name="C">
	      <short>English dictionary install source</short>
	      <long>Used for determining the path to install and update the English dictionary from.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/dictionary/kanji_source</key>
        <applyto>/apps/gwaei/dictionary/kanji_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@KANJI_DEFAULT_URI@</default>
        <locale name="C">
	      <short>Kanji dictionary install source</short>
	      <long>Used for determining the path to install and update the Kanji dictionary from.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/dictionary/names_source</key>
        <applyto>/apps/gwaei/dictionary/names_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@NAMES_DEFAULT_URI@</default>
        <locale name="C">
	      <short>Names dictionary install source</short>
	      <long>Used for determining the path to install and update the Names dictionary from.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/dictionary/radicals_source</key>
        <applyto>/apps/gwaei/dictionary/radicals_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@RADICALS_DEFAULT_URI@</default>
        <locale name="C">
	      <short>Radicals dictionary install source</short>
	      <long>Used for determining the path to install and update the Radicals dictionary from.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/dictionary/examples_source</key>
        <applyto>/apps/gwaei/dictionary/examples_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@EXAMPLES_DEFAULT_URI@</default>
        <locale name="C">
	      <short>Examples dictionary install source</short>
	      <long>Used for determining the path to install and update the Radicals dictionary from.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/gwaei/dictionary/french_source</key>
        <applyto>/apps/gwaei/dictionary/french_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@FRENCH_DEFAULT_URI@</default>
        <locale name="C">
	      <short>French dictionary install source</short>
	      <long>Used for determining the path to install and update the Radicals dictionary from.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/gwaei/dictionary/german_source</key>
        <applyto>/apps/gwaei/dictionary/german_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@GERMAN_DEFAULT_URI@</default>
        <locale name="C">
	      <short>German dictionary install source</short>
	      <long>Used for determining the path to install and update the Radicals dictionary from.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/gwaei/dictionary/spanish_source</key>
        <applyto>/apps/gwaei/dictionary/spanish_source</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@SPANISH_DEFAULT_URI@</default>
        <locale name="C">
	      <short>Spanish dictionary install source</short>
	      <long>Used for determining the path to install and update the Radicals dictionary from.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/gwaei/highlighting/comment_foreground</key>
        <applyto>/apps/gwaei/highlighting/comment_foreground</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@COMMENT_FG_DEFAULT@</default>
        <locale name="C">
	      <short>Color for comments in dictionary files</short>
	      <long>Is used to make the definitions stand out more from the comments.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/highlighting/match_foreground</key>
        <applyto>/apps/gwaei/highlighting/match_foreground</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@MATCH_FG_DEFAULT@</default>
        <locale name="C">
	      <short>Color used for query matches</short>
	      <long>Is used to make the matched portions of results more visible.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/highlighting/match_background</key>
        <applyto>/apps/gwaei/highlighting/match_background</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@MATCH_BG_DEFAULT@</default>
        <locale name="C">
	      <short>Color used for query matches</short>
	      <long>Is used to make the matched portions of results more visible.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/gwaei/highlighting/header_foreground</key>
        <applyto>/apps/gwaei/highlighting/header_foreground</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@HEADER_FG_DEFAULT@</default>
        <locale name="C">
	      <short>Used for the styling of some titles</short>
	      <long>Used as a separator for the "Other results" section.</long>
        </locale>
      </schema>

      <schema>
        <key>/schemas/apps/gwaei/highlighting/header_background</key>
        <applyto>/apps/gwaei/highlighting/header_background</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@HEADER_BG_DEFAULT@</default>
        <locale name="C">
	      <short>Used for the styling of some titles</short>
	      <long>Used as a separator for the "Other results" section. Is not always used.</long>
        </locale>
      </schema>


      <schema>
        <key>/schemas/apps/gwaei/dictionary/load_order</key>
        <applyto>/apps/gwaei/dictionary/load_order</applyto>
        <owner>gwaei</owner>
        <type>string</type>
        <default>@LOAD_ORDER_DEFAULT@</default>
        <locale name="C">
	      <short>Used for the styling of some titles</short>
	      <long>Used as a separator for the "Other results" section. Is not always used.</long>
        </locale>
      </schema>






   </schemalist>
</gconfschemafile>
