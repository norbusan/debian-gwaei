install-cglossaryDATA: $(cglossary_DATA)
	@$(NORMAL_INSTALL)
	@list='$(cglossary_DATA)'; test -n "$(cglossarydir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(cglossarydir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(cglossarydir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(cglossarydir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(cglossarydir)" || exit $$?; \
	done

uninstall-cglossaryDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(cglossary_DATA)'; test -n "$(cglossarydir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(cglossarydir)'; $(am__uninstall_files_from_dir)
